
[main]

wsLoadHostSql = webscan::select host from webscan_host_info where `status` =1 order by hasdetected desc,scanround asc limit 1000
setHasDetected = webscan::UPDATE webscan_host_info set HasDetected=? , scanRound = (scanRound + 1) where host=?
chechHostStatus = webscan::select * from webscan_host_info where host = ? and status = 1
setHostStatus = webscan::update webscan_host_info set status=? where host = ?
getUnInjected = webscan::select * from webscan_host_inject where type = 1 and host = ?
setToInjected = webscan::update webscan_host_inject set type = -1 where id = ?
getAllowsAllow = webscan::select url from webscan_host_url_allows where host = ? and status = 1
getNumberMatchHost = webscan::select host from webscan_host_info where status=-9

addHostParsedRecord = webscan::insert into webscan_source_detect( host, level_1_count, level_2_count, level_3_count, scan_num, path) values(?,?,?,?,?,?)
addPageParsedRecord = webscan::insert into webscan_page_score(host, score, checkby)values(?,?, 'testor')

addPageParsedRecordAttach = webscan::insert into webscan_page_detail(id,title,url,path)values(?,?,?,?)
checkAlreadyHasScored = webscan::select * from webscan_page_score where host = ? and urlhash=?

checkAllway = webscan::select * from webscan_host_url_allows where url = ?
insertAlways = webscan::insert into webscan_host_url_allows(host, url,type)values(?, ?,?)
